<html>
  <head>
    <title>task:min,mid,max </title>
  </head>
  <body>
    <script>
    document.write("Inputs:x=5 y=4 z=8");
    </script>
    <script>
      function min(){
      if(this.a>this.b){
      if(this.b>this.c){
      return this.c;}
      else{
      return this.b;}
      }
      else if(this.a>this.c){
      return this.c;}
      else{
      return this.a;}

      function mid(){
      if(this.a>this.b){
      if(this.b>this.c){
      return this.b;}
      else if(this.a>this.c){
      return this.c;}
      else{
      return this.a;}
      }
      else if(this.a>this.c){
      return this.a;}
      else if(this.b>this.c){
      return this.c;}
      else{
      return this.b}
      
      function max(){
      if(this.a>this.b){
      if(this.a>this.c){
      return this.a;}
      else{
      return this.c;}
      }
      else if(this.b>this.c){
      return this.b;}
      else{
      return this.c;}
      
      var x=5,y=4,z=8;
      var v= new Vec3(x,y,z);
      var min = v.min();
      var mid = v.mid();
      var max = v.max();
      document.write(min);
      document.write(mid);
      document.write(max);
    </script>
  </body>    
</html>
